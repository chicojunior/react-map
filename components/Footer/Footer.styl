@import '../App/App';

.Footer {
  background-color: #E21717

  &__Container {
    max-width: ContainerMaxWidth
    margin: 0 auto
    align-items: center
  }

  &__Location {
    a {
      color: inherit
      text-decoration: none
      display: inline-block
      padding: 10px
      font-style: normal
      font-family: 'Helvetica Neue', sans-serif
    }
  }

  .Navigator__Link {
    display: inline-block
    padding: 20px 10px
  }
}
